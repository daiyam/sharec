{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie > 10", "not ie > 0"]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    [
      "syntax-dynamic-import",
      {
        "foo": "baz"
      }
    ],
    "lodash"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-3"],
      "plugins": ["dynamic-import-node"]
    }
  },
  "ignore": ["*.babelrc", "*.vue"]
}
